@{
    ViewBag.Title = "NotFound";
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                Ir a  ---->
                <a href="http://localhost:49165/" title="Pagina1">Pagina Principal</a>.
            </p>
        </div>
    </section>
}
<h2>La informacion que busca no pudo ser encontrada</h2>
<h2>Detalles</h2>
<ul>
    <li><b>Codigo de Error: </b>@ViewBag.ErrorCode</li>
    <li><b>Descripcion: </b>@ViewBag.Description</li>
    <li><img src="~/Content/perr2.jpg" width="200" height="200" /></li>
</ul>
